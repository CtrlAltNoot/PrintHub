[server]
host: 0.0.0.0
port: 7125
enable_debug_logging: False
config_path: /home/klippy/klipper_config
log_path: /home/klippy/klipper_logs
database_path: ~/.moonraker_database
klippy_uds_address: /tmp/klippy_uds

[authorization]
trusted_clients:
    10.0.0.0/8
    127.0.0.0/8
    169.254.0.0/16
    172.21.0.0/12
    192.168.0.0/16
    192.168.1.0/24
    FE80::/10
    ::1/128
    https://prints.alchemycustoms.com
cors_domains:
    *.lan
    http://*.local
    https://*.local
    http://*.alchemylocal.com
    https://*.alchemylocal.com
    http://prints.alchemycustoms.com
    https://prints.alchemycustoms.com
    *://my.mainsail.xyz
    *://app.fluidd.xyz
force_logins: True

#[paneldue]
#serial: /dev/ttyUSB0
#baud: 57600
#machine_name: ACP01
#macros:
#  LOAD_FILAMENT
#  UNLOAD_FILAMENT
#  PANELDUE_BEEP FREQUENCY=500 DURATION=1
#confirmed_macros:
#  RESTART
#  FIRMWARE_RESTART

[octoprint_compat]

[history]

[update_manager]
enable_repo_debug: True
channel: dev

[update_manager mainsail]
type: web
repo: meteyou/mainsail
path: ~/mainsail

